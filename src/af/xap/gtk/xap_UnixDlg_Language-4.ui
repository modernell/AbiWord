<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <object class="GtkDialog" id="xap_UnixDlg_Language">
    <property name="width-request">300</property>
    <property name="height-request">330</property>
    <property name="title" translatable="yes">dialog1</property>
    <property name="type-hint">dialog</property>
    <child internal-child="content_area">
      <object class="GtkBox" id="dialog-vbox1">
        <property name="visible">0</property>
        <property name="orientation">vertical</property>
        <property name="spacing">12</property>
        <child>
          <object class="GtkBox" id="hbox1">
            <property name="visible">0</property>
            <child>
              <object class="GtkLabel" id="label1">
                <property name="visible">0</property>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="vbox1">
                <property name="visible">0</property>
                <property name="orientation">vertical</property>
                <property name="spacing">6</property>
                <child>
                  <object class="GtkLabel" id="lbAvailableLanguages">
                    <property name="visible">0</property>
                    <property name="label" translatable="yes">&lt;b&gt;Select Language&lt;/b&gt;</property>
                    <property name="use-markup">1</property>
                    <property name="xalign">0</property>
                  </object>
                </child>
                <child>
                  <object class="GtkBox" id="hbox2">
                    <property name="visible">0</property>
                    <child>
                      <object class="GtkLabel" id="label3">
                        <property name="visible">0</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox" id="vbox2">
                        <property name="visible">0</property>
                        <property name="orientation">vertical</property>
                        <property name="spacing">12</property>
                        <child>
                          <object class="GtkScrolledWindow" id="swAvailableLanguages">
                            <property name="visible">0</property>
                            <property name="can-focus">1</property>
                            <property name="hexpand">1</property>
                            <property name="vexpand">1</property>
                            <property name="shadow-type">in</property>
                            <child>
                              <object class="GtkTreeView" id="tvAvailableLanguages">
                                <property name="visible">0</property>
                                <property name="can-focus">1</property>
                                <property name="headers-visible">0</property>
                                <child internal-child="selection">
                                  <object class="GtkTreeSelection" id="treeview-selection1"/>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkBox" id="vbox3">
                            <property name="visible">0</property>
                            <property name="orientation">vertical</property>
                            <property name="spacing">6</property>
                            <child>
                              <object class="GtkLabel" id="lbDefaultLanguage">
                                <property name="visible">0</property>
                                <property name="label" translatable="yes">Default Language: </property>
                                <property name="xalign">0</property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkCheckButton" id="cbDefaultLanguage">
                                <property name="visible">0</property>
                                <property name="label" translatable="yes">Make default for document</property>
                                <property name="can-focus">1</property>
                                <property name="use-underline">1</property>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="label2">
                <property name="visible">0</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <action-widgets>
      <action-widget response="-7">btClose</action-widget>
    </action-widgets>
    <child internal-child="action_area">
      <object class="GtkBox" id="dialog-action_area1">
        <property name="visible">1</property>
        <child>
          <object class="GtkButton" id="btClose">
            <property name="visible">0</property>
            <property name="label">gtk-close</property>
            <property name="can-focus">1</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
