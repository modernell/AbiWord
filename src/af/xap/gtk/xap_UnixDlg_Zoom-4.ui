<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <object class="GtkAdjustment" id="adjustment1">
    <property name="lower">20</property>
    <property name="upper">500</property>
    <property name="value">100</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkDialog" id="xap_UnixDlg_Zoom">
    <property name="visible">1</property>
    <property name="title" translatable="yes">dialog1</property>
    <property name="resizable">0</property>
    <property name="type-hint">dialog</property>
    <child internal-child="content_area">
      <object class="GtkBox" id="dialog-vbox1">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkBox" id="vbox1">
            <property name="orientation">vertical</property>
            <property name="spacing">4</property>
            <child>
              <object class="GtkLabel" id="lbZoom">
                <property name="label" translatable="yes">&lt;b&gt;%s&lt;/b&gt;</property>
                <property name="use-markup">1</property>
                <property name="xalign">0</property>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="hbox1">
                <child>
                  <object class="GtkLabel" id="label2">
                  </object>
                </child>
                <child>
                  <object class="GtkBox" id="vbox2">
                    <property name="orientation">vertical</property>
                    <property name="spacing">6</property>
                    <child>
                      <object class="GtkRadioButton" id="rbPercent200">
                        <property name="label" translatable="yes">200%</property>
                        <property name="width-request">146</property>
                        <property name="can-focus">1</property>
                        <property name="use-underline">1</property>
                        <property name="active">1</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkRadioButton" id="rbPercent100">
                        <property name="label" translatable="yes">100%</property>
                        <property name="can-focus">1</property>
                        <property name="use-underline">1</property>
                        <property name="group">rbPercent200</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkRadioButton" id="rbPercent75">
                        <property name="label" translatable="yes">75%</property>
                        <property name="can-focus">1</property>
                        <property name="use-underline">1</property>
                        <property name="group">rbPercent200</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkRadioButton" id="rbPageWidth">
                        <property name="label" translatable="yes">Page width</property>
                        <property name="can-focus">1</property>
                        <property name="use-underline">1</property>
                        <property name="group">rbPercent200</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkRadioButton" id="rbWholePage">
                        <property name="label" translatable="yes">Whole page</property>
                        <property name="can-focus">1</property>
                        <property name="use-underline">1</property>
                        <property name="active">1</property>
                        <property name="group">rbPercent200</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkRadioButton" id="rbPercent">
                        <property name="label" translatable="yes">Percent:</property>
                        <property name="can-focus">1</property>
                        <property name="use-underline">1</property>
                        <property name="group">rbPercent200</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox" id="hbox2">
                        <child>
                          <object class="GtkLabel" id="label3">
                          </object>
                        </child>
                        <child>
                          <object class="GtkSpinButton" id="sbPercent">
                            <property name="can-focus">1</property>
                            <property name="adjustment">adjustment1</property>
                            <property name="climb-rate">1</property>
                            <property name="numeric">1</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <action-widgets>
      <action-widget response="-5">closebutton1</action-widget>
    </action-widgets>
    <child internal-child="action_area">
      <object class="GtkBox" id="dialog-action_area1">
        <property name="visible">1</property>
        <child>
          <object class="GtkButton" id="closebutton1">
            <property name="label">gtk-close</property>
            <property name="can-focus">1</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
