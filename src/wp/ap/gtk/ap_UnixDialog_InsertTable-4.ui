<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <object class="GtkAdjustment" id="adjustment1">
    <property name="lower">1</property>
    <property name="upper">64</property>
    <property name="value">5</property>
    <property name="step-increment">1</property>
    <property name="page-increment">5</property>
  </object>
  <object class="GtkAdjustment" id="adjustment2">
    <property name="lower">1</property>
    <property name="upper">2000</property>
    <property name="value">2</property>
    <property name="step-increment">1</property>
    <property name="page-increment">5</property>
  </object>
  <object class="GtkAdjustment" id="adjustment3">
    <property name="lower">0.10000000000000001</property>
    <property name="upper">500</property>
    <property name="value">0.69999999999999996</property>
    <property name="step-increment">0.10000000000000001</property>
    <property name="page-increment">1</property>
  </object>
  <object class="GtkDialog" id="ap_UnixDialog_InsertTable">
    <property name="visible">1</property>
    <property name="border-width">5</property>
    <property name="title" translatable="yes">Insert Table</property>
    <property name="resizable">0</property>
    <property name="modal">1</property>
    <property name="window-position">center-on-parent</property>
    <child internal-child="content_area">
      <object class="GtkBox" id="vbDialog">
        <property name="orientation">vertical</property>
        <property name="spacing">2</property>
        <child>
          <object class="GtkGrid" id="grContent">
            <property name="border-width">5</property>
            <property name="row-spacing">6</property>
            <property name="column-spacing">12</property>
            <child>
              <object class="GtkLabel" id="lbTableSize">
                <property name="label" translatable="yes">&lt;b&gt;%s&lt;/b&gt;</property>
                <property name="use-markup">1</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="left-attach">0</property>
                  <property name="top-attach">0</property>
                  <property name="column-span">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="lbNumCols">
                <property name="margin-left">18</property>
                <property name="label" translatable="yes">Number of columns:</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="left-attach">0</property>
                  <property name="top-attach">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="lbNumRows">
                <property name="margin-left">18</property>
                <property name="label" translatable="yes">Number of rows:</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="left-attach">0</property>
                  <property name="top-attach">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSpinButton" id="sbNumCols">
                <property name="width-request">64</property>
                <property name="can-focus">1</property>
                <property name="adjustment">adjustment1</property>
                <property name="climb-rate">1</property>
                <property name="numeric">1</property>
                <layout>
                  <property name="left-attach">1</property>
                  <property name="top-attach">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSpinButton" id="sbNumRows">
                <property name="can-focus">1</property>
                <property name="adjustment">adjustment2</property>
                <property name="climb-rate">1</property>
                <property name="numeric">1</property>
                <layout>
                  <property name="left-attach">1</property>
                  <property name="top-attach">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="lbAutoFit">
                <property name="margin-top">12</property>
                <property name="label" translatable="yes">&lt;b&gt;%s&lt;/b&gt;</property>
                <property name="use-markup">1</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="left-attach">0</property>
                  <property name="top-attach">3</property>
                  <property name="column-span">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkRadioButton" id="rbAutoColSize">
                <property name="label" translatable="yes">Automatic column size</property>
                <property name="can-focus">1</property>
                <property name="margin-left">18</property>
                <property name="use-underline">1</property>
                <property name="active">1</property>
                <layout>
                  <property name="left-attach">0</property>
                  <property name="top-attach">4</property>
                  <property name="column-span">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="lbInch">
                <property name="label" translatable="yes">inch</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="left-attach">2</property>
                  <property name="top-attach">5</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkRadioButton" id="rbFixedColSize">
                <property name="label" translatable="yes">Fixed column size:</property>
                <property name="can-focus">1</property>
                <property name="margin-left">18</property>
                <property name="use-underline">1</property>
                <property name="group">rbAutoColSize</property>
                <layout>
                  <property name="left-attach">0</property>
                  <property name="top-attach">5</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSpinButton" id="sbColSize">
                <property name="can-focus">1</property>
                <property name="adjustment">adjustment3</property>
                <property name="climb-rate">1</property>
                <property name="digits">1</property>
                <property name="numeric">1</property>
                <layout>
                  <property name="left-attach">1</property>
                  <property name="top-attach">5</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <action-widgets>
      <action-widget response="-6">btCancel</action-widget>
      <action-widget response="1">btInsert</action-widget>
    </action-widgets>
    <child internal-child="action_area">
      <object class="GtkButtonBox" id="aaDialog">
        <property name="visible">1</property>
        <property name="layout-style">end</property>
        <child>
          <object class="GtkButton" id="btCancel">
            <property name="label">gtk-cancel</property>
            <property name="can-focus">1</property>
            <property name="can-default">1</property>
            <property name="use-stock">1</property>
          </object>
          <packing>
            <property name="fill">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="btInsert">
            <property name="label" translatable="yes">_Insert</property>
            <property name="can-focus">1</property>
            <property name="can-default">1</property>
            <property name="has-default">1</property>
            <property name="use-underline">1</property>
          </object>
          <packing>
            <property name="fill">0</property>
            <property name="position">1</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
