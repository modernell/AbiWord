<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <object class="GtkDialog" id="ap_UnixDialog_Options_ColorSel">
    <property name="visible">1</property>
    <property name="border-width">5</property>
    <property name="title" translatable="yes">ap_UnixDialog_Options_ColorSel</property>
    <property name="resizable">0</property>
    <property name="window-position">center-on-parent</property>
    <property name="type-hint">dialog</property>
    <child internal-child="content_area">
      <object class="GtkBox" id="vbox1">
        <property name="orientation">vertical</property>
        <property name="spacing">2</property>
        <child>
          <object class="GtkColorChooserWidget" id="csColorSel">
            <property name="border-width">5</property>
          </object>
        </child>
      </object>
    </child>
    <action-widgets>
      <action-widget response="0">btnDefaults</action-widget>
      <action-widget response="-7">button1</action-widget>
    </action-widgets>
    <child internal-child="action_area">
      <object class="GtkButtonBox" id="hbbox1">
        <property name="visible">1</property>
        <property name="layout-style">end</property>
        <child>
          <object class="GtkButton" id="btnDefaults">
            <property name="label">gtk-revert-to-saved</property>
            <property name="can-focus">1</property>
            <property name="can-default">1</property>
            <property name="use-stock">1</property>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="button1">
            <property name="label">gtk-close</property>
            <property name="can-focus">1</property>
            <property name="can-default">1</property>
            <property name="use-stock">1</property>
          </object>
          <packing>
            <property name="position">1</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
