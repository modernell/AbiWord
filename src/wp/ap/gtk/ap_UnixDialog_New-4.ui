<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <object class="GtkDialog" id="ap_UnixDialog_New">
    <property name="visible">1</property>
    <property name="border-width">5</property>
    <property name="title" translatable="yes">dialog1</property>
    <property name="window-position">center-on-parent</property>
    <property name="type-hint">dialog</property>
    <child internal-child="content_area">
      <object class="GtkBox" id="dialog-vbox1">
        <property name="orientation">vertical</property>
        <property name="spacing">12</property>
        <child>
          <object class="GtkBox" id="hbox1">
            <child>
              <object class="GtkLabel" id="label2">
                <property name="margin-left">2</property>
                <property name="margin-right">2</property>
              </object>
              <packing>
                <property name="fill">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkBox" id="vbox6">
                <property name="orientation">vertical</property>
                <property name="spacing">12</property>
                <child>
                  <object class="GtkBox" id="vbox7">
                    <property name="orientation">vertical</property>
                    <child>
                      <object class="GtkRadioButton" id="rdTemplate">
                        <property name="label" translatable="yes">Create a new document from a template</property>
                        <property name="use-action-appearance">0</property>
                        <property name="can-focus">1</property>
                        <property name="use-underline">1</property>
                      </object>
                      <packing>
                        <property name="fill">0</property>
                        <property name="padding">4</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkBox" id="hbox2">
                        <child>
                          <object class="GtkLabel" id="label4">
                            <property name="xpad">6</property>
                          </object>
                          <packing>
                            <property name="fill">0</property>
                          </packing>
                        </child>
                        <child>
                          <object class="GtkScrolledWindow" id="swTemplates">
                            <property name="can-focus">1</property>
                            <property name="shadow-type">in</property>
                            <child>
                              <object class="GtkTreeView" id="tvTemplates">
                                <property name="width-request">160</property>
                                <property name="height-request">160</property>
                                <property name="sensitive">0</property>
                                <property name="can-focus">1</property>
                                <property name="headers-visible">0</property>
                                <child internal-child="selection">
                                  <object class="GtkTreeSelection" id="treeview-selection1"/>
                                </child>
                              </object>
                            </child>
                          </object>
                          <packing>
                            <property name="expand">1</property>
                          </packing>
                        </child>
                      </object>
                      <packing>
                        <property name="expand">1</property>
                      </packing>
                    </child>
                  </object>
                  <packing>
                    <property name="expand">1</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkBox" id="vbox8">
                    <property name="orientation">vertical</property>
                    <child>
                      <object class="GtkRadioButton" id="rdOpen">
                        <property name="label" translatable="yes">Open an existing file</property>
                        <property name="use-action-appearance">0</property>
                        <property name="can-focus">1</property>
                        <property name="use-underline">1</property>
                        <property name="group">rdTemplate</property>
                      </object>
                      <packing>
                        <property name="fill">0</property>
                        <property name="padding">4</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkBox" id="hbox3">
                        <child>
                          <object class="GtkLabel" id="label5">
                            <property name="xpad">6</property>
                          </object>
                          <packing>
                            <property name="fill">0</property>
                          </packing>
                        </child>
                        <child>
                          <object class="GtkButton" id="btFile">
                            <property name="label">gtk-open</property>
                            <property name="use-action-appearance">0</property>
                            <property name="sensitive">0</property>
                            <property name="can-focus">1</property>
                            <property name="use-stock">1</property>
                          </object>
                          <packing>
                            <property name="expand">1</property>
                          </packing>
                        </child>
                      </object>
                      <packing>
                        <property name="expand">1</property>
                      </packing>
                    </child>
                  </object>
                </child>
              </object>
              <packing>
                <property name="expand">1</property>
              </packing>
            </child>
            <child>
              <object class="GtkLabel" id="label3">
                <property name="margin-left">2</property>
                <property name="margin-right">2</property>
              </object>
              <packing>
                <property name="fill">0</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">1</property>
          </packing>
        </child>
      </object>
    </child>
    <action-widgets>
      <action-widget response="-6">btCancel</action-widget>
      <action-widget response="-5">btOK</action-widget>
    </action-widgets>
    <child internal-child="action_area">
      <object class="GtkButtonBox" id="dialog-action_area1">
        <property name="visible">1</property>
        <property name="layout-style">end</property>
        <child>
          <object class="GtkButton" id="btCancel">
            <property name="label">gtk-cancel</property>
            <property name="can-focus">1</property>
            <property name="can-default">1</property>
            <property name="use-stock">1</property>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="btOK">
            <property name="label">gtk-ok</property>
            <property name="can-focus">1</property>
            <property name="can-default">1</property>
            <property name="use-stock">1</property>
          </object>
          <packing>
            <property name="position">1</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
