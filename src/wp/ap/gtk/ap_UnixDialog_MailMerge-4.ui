<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <object class="GtkDialog" id="ap_UnixDialog_MailMerge">
    <property name="visible">1</property>
    <property name="border-width">5</property>
    <property name="title" translatable="yes">Insert Mail Merge Field</property>
    <property name="window-position">mouse</property>
    <property name="default-width">400</property>
    <property name="default-height">250</property>
    <child internal-child="content_area">
      <object class="GtkBox" id="vbDialog">
        <property name="orientation">vertical</property>
        <property name="spacing">2</property>
        <child>
          <object class="GtkGrid" id="grContent">
            <property name="vexpand">1</property>
            <property name="border-width">5</property>
            <property name="row-spacing">6</property>
            <child>
              <object class="GtkLabel" id="lbAvailableFields">
                <property name="label" translatable="yes">&lt;b&gt;%s&lt;/b&gt;</property>
                <property name="use-markup">1</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="left-attach">0</property>
                  <property name="top-attach">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkScrolledWindow" id="swAvailableFields">
                <property name="can-focus">1</property>
                <property name="margin-left">18</property>
                <property name="hexpand">1</property>
                <property name="vexpand">1</property>
                <property name="shadow-type">in</property>
                <child>
                  <object class="GtkTreeView" id="tvAvailableFields">
                    <property name="can-focus">1</property>
                    <property name="headers-visible">0</property>
                    <property name="reorderable">1</property>
                    <child internal-child="selection">
                      <object class="GtkTreeSelection" id="treeview-selection1"/>
                    </child>
                  </object>
                </child>
                <layout>
                  <property name="left-attach">0</property>
                  <property name="top-attach">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="lbFieldName">
                <property name="margin-top">12</property>
                <property name="label" translatable="yes">&lt;b&gt;%s&lt;/b&gt;</property>
                <property name="use-markup">1</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="left-attach">0</property>
                  <property name="top-attach">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkEntry" id="edFieldName">
                <property name="can-focus">1</property>
                <property name="margin-left">18</property>
                <layout>
                  <property name="left-attach">0</property>
                  <property name="top-attach">3</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <action-widgets>
      <action-widget response="2">btOpenFile</action-widget>
      <action-widget response="-7">btClose</action-widget>
      <action-widget response="1">btInsert</action-widget>
    </action-widgets>
    <child internal-child="action_area">
      <object class="GtkButtonBox" id="aaDialog">
        <property name="visible">1</property>
        <property name="layout-style">end</property>
        <child>
          <object class="GtkButton" id="btOpenFile">
            <property name="can-focus">1</property>
            <property name="can-default">1</property>
            <child>
              <object class="GtkAlignment" id="alOpenFile">
                <property name="visible">1</property>
                <property name="xscale">0</property>
                <property name="yscale">0</property>
                <child>
                  <object class="GtkBox" id="hbOpenFile">
                    <property name="spacing">2</property>
                    <child>
                      <object class="GtkImage" id="imOpenFile">
                        <property name="stock">gtk-open</property>
                      </object>
                      <packing>
                        <property name="fill">0</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkLabel" id="lbOpenFile">
                        <property name="label" translatable="yes">_Open File</property>
                        <property name="use-underline">1</property>
                      </object>
                      <packing>
                        <property name="fill">0</property>
                      </packing>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="btClose">
            <property name="label">gtk-close</property>
            <property name="can-focus">1</property>
            <property name="can-default">1</property>
            <property name="use-stock">1</property>
          </object>
          <packing>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="btInsert">
            <property name="label" translatable="yes">_Insert</property>
            <property name="can-focus">1</property>
            <property name="can-default">1</property>
            <property name="has-default">1</property>
            <property name="use-underline">1</property>
          </object>
          <packing>
            <property name="position">2</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
